PORT aux_clk2x180   = "", DIR = OUT, SIGIS = CLK, CLK_INPORT = aux0_clk_p, CLK_FACTOR = 2
PORT aux_clk2x270   = "", DIR = OUT, SIGIS = CLK, CLK_INPORT = aux0_clk_p, CLK_FACTOR = 2
